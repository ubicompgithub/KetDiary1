<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id = "@+id/statistic_week_layout" 
    >

    <TextView
        android:id="@+id/statistic_week_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/week_test"
        android:textSize="@dimen/small_title_size"
        android:layout_marginTop="36dp"
        android:layout_marginLeft="30dp"
        android:layout_marginBottom="15dp"
        android:textColor="@color/text_gray2" />
    
    <LinearLayout
        android:id="@+id/statistic_week_date_label_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="30dp"
        android:layout_below="@+id/statistic_week_title"
        
        />
    
    
    <ImageView
        android:id="@+id/statistic_proscons_background"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="2dp"
        android:src="@drawable/statistic_proscons_back" 
        android:layout_below="@+id/statistic_week_date_label_layout"/>
    
	<GridLayout 
	    android:id = "@+id/statistic_week_block_layout"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:columnCount="7"
	    android:layout_marginLeft="30dp"
	    android:layout_below="@+id/statistic_week_date_label_layout"
	    
		 />
	<GridLayout 
	    android:id = "@+id/statistic_week_link_layout"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:columnCount="7"
	    android:layout_marginLeft="48dp"
	    android:layout_below="@+id/statistic_week_date_label_layout"
	    
		 />
		
    	<LinearLayout
        android:id="@+id/statistic_week_timeblock_label_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="30dp"
        android:layout_below="@+id/statistic_week_block_layout"
        />
	
    
    	<LinearLayout
        android:id="@+id/statistic_week_label_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/statistic_week_timeblock_label_layout"
        />
    
</RelativeLayout>